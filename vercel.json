{
  "version": 2,
  "builds": [{"src" : "app.js", "use": "@now/node-server"}],
  "routes": [
    {
      "src": "/allpost",
      "dest": "/app.js",
      "methods": ["GET"]
    },
    {
      "src": "/users",
      "dest": "/app.js",
      "methods": ["GET"]
    },
    {
      "src": "/user/:id",
      "dest": "/app.js",
      "methods": ["GET"]
    },
    {
      "src": "/follow",
      "dest": "/app.js",
      "methods": ["PUT"]
    }
  ]
}